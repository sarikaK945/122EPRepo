<?xml version="1.0" encoding="UTF-8"?>
<xsl:stylesheet version="1.0" xmlns:datetime="http://exslt.org/dates-and-times" xmlns:set="http://exslt.org/sets" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsl="http://www.w3.org/1999/XSL/Transform" xmlns:math="http://exslt.org/math" xmlns:lxslt="http://xml.apache.org/xslt" xmlns:xalan="http://xml.apache.org/xslt" xmlns:java="http://xml.apache.org/xslt/java" exclude-result-prefixes="xalan java lxslt" xmlns:ns1="http://www.fiorano.com/b2b/host/trigger" xmlns:ns3="http://www.fiorano.com/fesb/activity/POP31/In" xmlns:ns4="http://www.w3.org/2001/XMLSchema" xmlns:dateConversion="ext1" xmlns:myext="ext2" xmlns:strings="ext3" extension-element-prefixes="datetime set math dateConversion myext strings">
  <xsl:output method="xml" indent="yes" encoding="UTF-8" xalan:indent-amount="2" omit-xml-declaration="no"></xsl:output>
  <xsl:variable name="document" select="/"></xsl:variable>
  <xsl:variable name="document_OUT_PORT" select="document('OUT_PORT')"></xsl:variable>
  <xsl:template match="/">
    <ns3:Input>
      <MessageCount>
        <xsl:value-of select="java:setStringProperty($_TIF_MESSAGE_, 'trigger', 'true')"></xsl:value-of>
        <xsl:value-of select="java:setStringProperty($_TIF_MESSAGE_, 'action', $document_OUT_PORT/ns1:Channel/ns1:Action)"></xsl:value-of>
        <xsl:value-of select="java:setStringProperty($_TIF_MESSAGE_, 'B2BISConfigNames', $document_OUT_PORT/ns1:Channel/ns1:INTRCONFIGID)"></xsl:value-of>
        <xsl:value-of select="java:setStringProperty($_TIF_MESSAGE_, 'B2BMISCConfigNames', $document_OUT_PORT/ns1:Channel/ns1:MISCCONFIGID)"></xsl:value-of>
        <xsl:value-of select="java:setStringProperty($_TIF_MESSAGE_, 'B2BMCFConfigNames', $document_OUT_PORT/ns1:Channel/ns1:CONNCONFIGID)"></xsl:value-of>
      </MessageCount>
    </ns3:Input>
  </xsl:template>
<!--The list of parameters defined by user-->
  <xsl:param name="_TIF_MESSAGE_"></xsl:param>
</xsl:stylesheet>
